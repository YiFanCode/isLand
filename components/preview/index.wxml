<view class="container" bindtap="onTap">
  <view class="head">
    <cmp-tag text="{{typeText}}" tag-class="tag" />
    <cmp-like class="like" read-only="{{true}}" like="{{true}}" count="{{classic.fav_nums}}" />
  </view>
  <image class="{{classic.type==200 ? 'music-img' : 'other-img'}}" src="{{classic.image}}"></image>
  <view class="text">{{classic.content}}</view>
</view>
